class LtdPaperDetails{
    string id;
    string src; // which service provides the data for this paper
    string title;
    string[] authors;
    int pubYear;
    int citedCount;
}

class XtdPaperDetails{
    string id;
    string src; // which service provides the data for this paper
    string title;
    string[] authors;
    int pubYear;
    int citedCount;
    map<string, int> TxtMndTrmsCount; // text mined terms present in the paper and their number of occurence
}